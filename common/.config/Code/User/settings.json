{
    "window.zoomLevel": 1,
    "editor.fontFamily": "'Hack Nerd Font','Fira Code','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "extensions.ignoreRecommendations": false,
    "workbench.startupEditor": "newUntitledFile",
    "editor.fontSize": 16,
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    "terminal.external.linuxExec": "tilix",
    "explorer.openEditors.visible": 0,
    "editor.formatOnPaste": false,
    "files.autoSave": "off",
    "vsicons.projectDetection.autoReload": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "workbench.colorTheme": "Material Theme Darker",
    "diffEditor.ignoreTrimWhitespace": true,
    "breadcrumbs.enabled": true,
    "liveServer.settings.ChromeDebuggingAttachment": true,
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    // "liveServer.settings.AdvanceCustomBrowserCmdLine": "/usr/bin/chromium-browser --remote-debugging-port=9222",
    // "workbench.settings.editor": "json",
}